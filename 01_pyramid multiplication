#include <stdio.h>

int main() {
    int num;
    printf("Enter your Number for its multiplication table: ");
    scanf("%d", &num);
    for (int i = 1; i <= 10; i++) {  
        for (int j = 1; j <= i; j++) {
            printf("%d ", num * j);
        }
        printf("\n");
    }
    return 0;
}
